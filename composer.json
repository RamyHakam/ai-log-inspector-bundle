{
  "name": "hakam/ai-log-inspector-bundle",
  "description": "AI-powered Symfony bundle to inspect, analyze, and interact with logs using LLMs and MCP agents.",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Ramy Hakam",
      "email": "pencilsoft1@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "symfony/framework-bundle": "^6.4 || ^7.0",
    "symfony/config": "^6.4 || ^7.0",
    "symfony/dependency-injection": "^6.4 || ^7.0",
    "symfony/console": "^6.4 || ^7.0",
    "hakam/ai-log-inspector-agent": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "Hakam\\AiLogInspectorBundle\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../ai-log-inspector-agent"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "symfony/test-pack": "^1.2",
    "dg/bypass-finals": "^1.9"
  }
}
